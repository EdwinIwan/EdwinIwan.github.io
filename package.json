{
  "name": "Blog",
  "description": "Blog",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "clean": "rm -rf ./__sapper__",
    "export": "sapper export --legacy",
    "fix": "npx run-p \"format:* --write\" \"lint:* --fix\"",
    "lint:svelte": "npx eslint \"src/**/*.svelte\"",
    "lint:ts": "npx eslint \"src/**/*.ts\"",
    "lint:js": "npx eslint \"./**/*.js\"",
    "format:svelte": "npx prettier --write --plugin-search-dir=. ./**/*.svelte",
    "format:ts": "npx prettier \"src/**/*.ts\"",
    "format:js": "npx prettier \"**/*.js\"",
    "deploy": "npm run clean && npm run export && node ./scripts/gh-pages.js"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@types/node": "^14.0.6",
    "eslint": "^7.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.31",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "sapper": "^0.27.13",
    "svelte": "^3.23.0",
    "svelte-preprocess": "^3.7.4",
    "typescript": "^3.9.3"
  }
}
